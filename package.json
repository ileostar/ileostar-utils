{
  "name": "ileostar-utils",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "commit": "git add . && git-cz",
    "push": "git push",
    "fix": "eslint src/**/*.*  --fix"
  },
  "keywords": [],
  "author": "ileostar",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "gitHooks": {
    "commit-msg": "node ./scripts/verifyCommitMsg.ts"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
    "eslint --fix",
    "git add ."
    ]
  },
  "devDependencies": {
    "@ileostar/eslint-config": "^0.3.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.14.0",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "cz-emoji": "1.3.2-canary.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/node": "^20.8.4"
  }
}
